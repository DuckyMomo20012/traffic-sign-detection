{
  "env": {
    "browser": true,
    "es2022": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "plugin:import/recommended",
    "plugin:prettier/recommended"
  ],
  "plugins": ["import"],
  "rules": {
    "eqeqeq": "warn",
    "import/extensions": "off",
    "import/no-unresolved": "off",
    "import/prefer-default-export": "off",
    "no-var": "error",
    "prefer-destructuring": [
      "warn",
      {
        "array": false,
        "object": true
      }
    ],
    "no-param-reassign": ["off"],
    "prettier/prettier": [
      "error",
      {
        "endOfLine": "auto"
      }
    ],
    "require-await": "warn",
    "spaced-comment": ["warn", "always"],
    "strict": ["error", "global"]
  }
}
