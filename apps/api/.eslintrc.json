{
  "extends": ["../../.eslintrc.json"],
  "plugins": ["import"],
  "rules": {
    "import/no-extraneous-dependencies": [
      "warn",
      {
        "devDependencies": true,
        "packageDir": ["./"]
      }
    ]
  },
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "rules": {}
    },
    {
      "files": ["*.ts", "*.tsx"],
      "rules": {}
    },
    {
      "files": ["*.js", "*.jsx"],
      "rules": {}
    }
  ]
}
